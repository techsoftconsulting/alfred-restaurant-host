{
  "expo": {
    "name": "Alfred Restaurant Host",
    "slug": "alfred-restaurant-host-app",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "userInterfaceStyle": "light",
    "orientation": "portrait",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 30000
    },
    "splash": {
      "resizeMode": "cover",
      "hideExponentText": false,
      "backgroundColor": "#4979d0"
    }
  }
}


